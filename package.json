    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  },
  "dependencies": {
    "express": "^5.0.1",
    "http-errors": "^2.0.0",
    "postgres": "^3.4.5",
    "zod": "^3.23.8"
  }
}
